
  CREATE TABLE "DB7"."A_BOARD" 
   (	"BOARD_NUM" NUMBER, 
	"MEM_NUM" NUMBER, 
	"MEM_PHOTO" VARCHAR2(100 BYTE), 
	"MEM_NICKNAME" VARCHAR2(20 BYTE), 
	"BOARD_CONTENT" VARCHAR2(4000 BYTE), 
	"BOARD_DATE" DATE, 
	"BOARD_SECRET" NUMBER(10,0), 
	"BOARD_READ_COUNT" NUMBER, 
	"RE_NUM" NUMBER, 
	"LIKE_NUM" NUMBER, 
	"BOARD_TYPE" NUMBER(10,0), 
	"FOLDER_NUM" NUMBER, 
	"BOARD_FILE" VARCHAR2(100 BYTE), 
	 CONSTRAINT "PK_BOARD_BOARD_NUM" PRIMARY KEY ("BOARD_NUM")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "USERS"  ENABLE, 
	 CONSTRAINT "FK_BOARD_MEM_NUM" FOREIGN KEY ("MEM_NUM")
	  REFERENCES "DB7"."A_MEMBER" ("MEM_NUM") ENABLE, 
	 CONSTRAINT "FK_BOARD_MEM_NICKNAME" FOREIGN KEY ("MEM_NICKNAME")
	  REFERENCES "DB7"."A_MEMBER" ("MEM_NICKNAME") ENABLE, 
	 CONSTRAINT "FK_BOARD_RE_NUM" FOREIGN KEY ("RE_NUM")
	  REFERENCES "DB7"."A_RE" ("RE_NUM") ENABLE, 
	 CONSTRAINT "FK_BOARD_LIKE_NUM" FOREIGN KEY ("LIKE_NUM")
	  REFERENCES "DB7"."A_LIKE" ("LIKE_NUM") ENABLE, 
	 CONSTRAINT "FK_FOLDER_NUM" FOREIGN KEY ("FOLDER_NUM")
	  REFERENCES "DB7"."A_FOLDER" ("FOLDER_NUM") ON DELETE CASCADE ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "USERS" ;
--------------------------



